<?php

/**
 * @file
 * Contains prueba_import.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function prueba_import_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the prueba_import module.
    case 'help.page.prueba_import':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Prueba Import') . '</p>';
      return $output;
    
    default:
  }
}
